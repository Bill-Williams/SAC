@model SAC.Domain.Models.Schedule

@{
    ViewBag.Title = "Delete";
}

<div class="well">
    <h2>@ViewBag.Title</h2>
    <h4>Are you sure you want to delete this?</h4>
</div>

<div>
    <h4>Schedule</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.DisplayShortDate)
        </dt>
        
        <dd>
            @Html.DisplayFor(model => model.DisplayShortDate)
        </dd>
 
        <dt>
            @Html.DisplayNameFor(model => model.Club)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Club.Name)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.BasicDescription)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.BasicDescription)
        </dd>
    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            @Html.Partial("BtnDeleteCancelReturn", Model)
        </div>
    }
</div>
